local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/GreenDeno/Venyx-UI-Library/main/source.lua"))()
local venyx = library.new("DXLR", 5013109572)

local themes = {
Background = Color3.fromRGB(24, 24, 24),
Glow = Color3.fromRGB(0, 0, 0),
Accent = Color3.fromRGB(10, 10, 10),
LightContrast = Color3.fromRGB(20, 20, 20),
DarkContrast = Color3.fromRGB(14, 14, 14),  
TextColor = Color3.fromRGB(255, 255, 255)
}

local page = venyx:addPage("Main", 5012544693)
local section1 = page:addSection("Legends Of Speed")


local head = game:GetService("Players").LocalPlayer.Character:FindFirstChild("Head")

local function collectorbs()
    for i,v in pairs(game:GetService("Workspace").Coins:GetChildren()) do
        if v.Name == "Coin" then
            for _,p in pairs(v:GetChildren()) do
                if p.Name == "TouchInterest" and p.Parent then 
        firetouchinterest(head, p.Parent, 0)
        task.wait(0.2)
        firetouchinterest(head, p.Parent, 1)
    end
end
end
    end
end

local lplr = game:GetService("Players").LocalPlayer
local lroot = lplr.Character:FindFirstChild("HumanoidRootPart")
local function autofarm()
    if lplr.Character:FindFirstChild("HumanoidRootPart") then
game:GetService("TweenService"):Create(lroot, TweenInfo.new(5, Enum.EasingStyle.Linear), {CFrame = game:GetService("Workspace").Tiles.StudLine9214.CFrame}):Play()
task.wait(2)
    end
end


local heartbeat 
section1:addToggle("Auto Collect Coins", nil, function(value)
if value == true then  
           heartbeat = game:GetService("RunService").Heartbeat:Connect(function()
            collectorbs()
        end)
else
    heartbeat:Disconnect() 
end
end)

local heartbeat2
section1:addToggle("Auto farm", nil, function(value)
if value == true then  
           heartbeat2 = game:GetService("RunService").Heartbeat:Connect(function()
            autofarm()
        end)
else
    heartbeat2:Disconnect() 
end
end)


section1:addButton("Collect All Coins", function()
    collectorbs()
end)


section1:addButton("Auto Farm", function()
    autofarm()
end)

venyx:SelectPage(venyx.pages[1], true)

